{
  "name": "sd-project-backend",
  "version": "1.0.0",
  "description": "Backend API",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "pretest": "cross-env NODE_ENV=test node src/scripts/reset-test-db.js",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --testTimeout=30000 --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --testTimeout=30000",
    "test:coverage": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --testTimeout=30000",
    "test:integration": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --testTimeout=30000 --testPathPattern=src/tests/integration",
    "test:auth": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --testTimeout=30000 --testPathPattern=src/tests/integration/auth",
    "test:producerProfile": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --testTimeout=30000 --testPathPattern=src/tests/integration/producerProfile.test.js",
    "test:db:reset": "cross-env NODE_ENV=test node src/scripts/reset-test-db.js",
    "migrate": "cross-env NODE_ENV=development node src/scripts/migrate-ordered.js",
    "seed": "cross-env NODE_ENV=development node src/scripts/migrate-ordered.js --seed",
    "migrate:test": "cross-env NODE_ENV=test node src/scripts/migrate-ordered.js",
    "seed:test": "cross-env NODE_ENV=test node src/scripts/migrate-ordered.js --seed",
    "verify-config": "node src/scripts/verify-config.js"
  },
  "keywords": [],
  "author": "kai",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.1",
    "mysql2": "^3.2.0",
    "node-fetch": "^3.3.2",
    "sequelize": "^6.31.0",
    "slugify": "^1.6.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@eslint/eslintrc": "^3.3.1",
    "babel-jest": "^29.7.0",
    "bcrypt": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "supertest": "^7.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
