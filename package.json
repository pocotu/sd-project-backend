{
  "name": "sd-project-backend",
  "version": "1.0.0",
  "description": "Backend API para proyecto universitario",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/tests/integration",
    "test:integration:auth": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/tests/integration/auth",
    "test:integration:phase1": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/tests/integration/phase/phase1",
    "test:integration:phase2": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/tests/integration/phase/phase2",
    "test:db": "node src/database/test-migrations.js",
    "test:phase2": "node src/database/test-phase2.js",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all"
  },
  "keywords": [],
  "author": "kai",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.0",
    "node-fetch": "^3.3.2",
    "sequelize": "^6.31.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "supertest": "^7.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
